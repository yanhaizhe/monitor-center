<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.beingmate.monitorcenter.dal.cat.dao.ConfigDOMapper">
    <resultMap id="BaseResultMap" type="com.beingmate.monitorcenter.dal.cat.dataobject.ConfigDO">
        <id column="id" jdbcType="INTEGER" property="id"/>
        <result column="name" jdbcType="VARCHAR" property="name"/>
        <result column="creation_date" jdbcType="TIMESTAMP" property="creationDate"/>
        <result column="modify_date" jdbcType="TIMESTAMP" property="modifyDate"/>
    </resultMap>
    <sql id="Base_Column_List">
    id, name, creation_date, modify_date
  </sql>

    <select id="selectByPrimaryKey" parameterType="java.lang.Integer" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from config
        where id = #{id,jdbcType=INTEGER}
    </select>

    <select id="listConfigByName" parameterType="java.lang.String" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from config
        where name = #{name,jdbcType=VARCHAR}
        ORDER BY id
    </select>
</mapper>